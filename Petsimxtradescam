import requests
import random
import string
import json

# Function to generate random string of given length
def generate_random_string(length):
    letters = string.ascii_letters
    return ''.join(random.choice(letters) for i in range(length))

# Generate random username and password
username = generate_random_string(10)
password = generate_random_string(10)

# Generate random name and Gmail address
name = generate_random_string(8)
email = name + "@gmail.com"

# API endpoint for registration
register_url = "https://easy-earn.homes/416849468261/register"

# Prepare the request payload
payload = {
    "username": username,
    "password": password,
    "email": email,
    "full_name": name
}

# Send the POST request to register the account
response = requests.post(register_url, json=payload)

# Handle the response
if response.status_code == 200:
    print("Account registered successfully!")
    print("Username:", username)
    print("Password:", password)
    print("Name:", name)
    print("Email:", email)

    # Save account details to a JSON file
    account_details = {
        "username": username,
        "password": password,
        "name": name,
        "email": email
    }
    with open('account_details.json', 'w') as file:
        json.dump(account_details, file, indent=4)
        print("Account details saved to account_details.json")
else:
    print("Failed to register the account.")
